{
    "name": "lambda",
    "version": "0.0.1",
    "description": "A tool for aggregating multiple GW2EI parsed combat logs and for at-a-glance insights",
    "main": "index.js",
    "scripts": {
        "start:dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
        "test": "jest ",
        "lint": "eslint src/**/*.ts",
        "build": "tsc --noEmit && node build.cjs",
        "build:executable": "pkg -t linux,macos,win src/index.ts --out-path dist",
        "format": "eslint src/**/*.ts --fix"
    },
    "private": true,
    "author": "Aria J",
    "license": "GPL-3.0-or-later",
    "dependencies": {
        "adm-zip": "^0.5.10",
        "aws-lambda": "^1.0.7",
        "aws-sdk": "^2.1363.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "helmet": "^5.0.2",
        "husky": "^7.0.4",
        "jsdom": "^19.0.0",
        "multer": "^1.4.4",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.5.0",
        "@types/aws-lambda": "^8.10.114",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/jsdom": "^16.2.14",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.24",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "esbuild": "^0.17.18",
        "eslint": "^8.13.0",
        "jest": "^27.5.1",
        "prettier": "^2.6.2",
        "ts-jest": "^27.1.4",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.3"
    }
}
